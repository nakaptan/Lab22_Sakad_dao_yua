{
    "version": "2.0.0",
    "tasks": [
      {
        "label": "Build C++ (GCC 14)",
        "type": "shell",
        "command": "/opt/homebrew/bin/g++-14",
        "args": [
          "-std=c++17",
          "-o",
          "${workspaceFolder}/lab22",
          "${workspaceFolder}/lab22.cpp"
        ],
        "group": {
          "kind": "build",
          "isDefault": true
        },
        "problemMatcher": ["$gcc"]
      }
    ]
  }